{
  "name": "assets",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "module": "./src/index.ts",
      "require": "./lib/index.js",
      "types": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "crypto-icons:clean": "svgo -f data/images/erc20 -o data/images/erc20 --final-newline --eol lf && tsx scripts/lowercase-files.ts data/images/erc20",
    "crypto-icons:upload": "tsx scripts/upload-to-aws.ts",
    "format": "prettier --cache --log-level warn --check .",
    "format:fix": "prettier --cache --log-level warn --write .",
    "format:fix:nocache": "prettier --log-level warn --write .",
    "generate-erc20-tokens": "tsx scripts/generate-erc20-tokens.ts && prettier --log-level warn --write src/constants/erc20Tokens.json src/types/supported-symbols.ts src/constants/erc20Lookup.ts",
    "lint": "concurrently \"pnpm format\" \"pnpm lint:ts\" \"pnpm type-check\"",
    "lint:fix": "concurrently \"pnpm format:fix\" \"pnpm lint:ts:fix\" \"pnpm type-check\"",
    "lint:fix:nocache": "concurrently \"pnpm format:fix:nocache\" \"pnpm lint:ts:fix:nocache\" \"pnpm type-check\"",
    "lint:ts": "eslint --cache --quiet .",
    "lint:ts:fix": "eslint --cache --quiet --fix .",
    "lint:ts:fix:nocache": "eslint --quiet --fix .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "pnpm type-check --watch"
  },
  "dependencies": {
    "chains": "workspace:*",
    "common": "workspace:*",
    "contracts-internal": "workspace:*",
    "viem": "^2.9.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@types/glob": "^8.1.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-astaria": "workspace:*",
    "glob": "^10.3.12",
    "prettier": "^3.3.3",
    "prettier-config-astaria": "workspace:*",
    "svgo": "^3.2.0",
    "tsconfig": "workspace:*",
    "tsx": "^4.7.3"
  }
}

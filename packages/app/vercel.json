{
  "crons": [
    {
      "path": "/api/cron/warmAssetMetadata",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/matchingservice?chainId=8453&chainId=34443",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/clearInvalidIntents?chainId=8453&chainId=34443&chainId=11155111",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/cron/leaderboard?snapshot=1708473600",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/intentNotifications",
      "schedule": "0 */12 * * *"
    },
    {
      "path": "/api/cron/marketData?forSepolia=false",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/populatePointTokenPrices?chainId=8453&chainId=34443&chainId=11155111",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/ethBalanceMatchingServiceWallet?chainId=8453&chainId=34443&chainId=11155111",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/intentsUsdBatchPricing?forSepolia=false",
      "schedule": "*/10 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://export.turnkey.com  https://verify.walletconnect.org https://verify.walletconnect.com https://secure.walletconnect.com https://secure.walletconnect.org https://api.management.inkeep.com https://api.inkeep.com https://uikit.inkeep.com;"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), camera=(), fullscreen=*, payment=self"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/claim-with-gas",
      "destination": "https://faucet-ashy.vercel.app/claim-with-gas/"
    }
  ]
}
